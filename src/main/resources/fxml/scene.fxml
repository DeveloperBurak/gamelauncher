<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="scene" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ui.FXMLController">
    <Button fx:id="expandButton" onAction="#centered"/>


    <StackPane fx:id="expandedScene" onMouseExited="#collapseScreen" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <VBox fx:id="gamesList" maxWidth="250">
            <Label fx:id="labelGameList" text="Game List"/>


        </VBox>
    </StackPane>
</AnchorPane>